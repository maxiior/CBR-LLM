{
  "prompt": {
    "file_name": "prompt_1",
    "tags": ["<NAME>", "<STEPS>", "<INGREDIENTS>"]
  },
  "model": {
    "type": "gpt",
    "name": "gpt-3.5-turbo-instruct",
    "params": {
      "temperature": 0,
      "response_format": "json"
    }
  },
  "datasets": {
    "file_name": "RAW_recipes.csv",
    "columns_to_extract": [
      "name",
      "n_steps",
      "steps",
      "ingredients",
      "n_ingredients"
    ],
    "mask_tag": "<MASK>",
    "proportions": {
      "train": 0.25,
      "valid": 0.25,
      "test": 0.25,
      "langchain": 0.25
    }
  }
}
